{
  "name": "autoketing-dashboard-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "react-redux": "^5.0.7",
    "prop-types": "^15.6.1",
    "@shopify/polaris": "^2.0.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "history": "^4.7.2",
    "bootstrap": "^4.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.17.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.43.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.6.1",
    "npm-run-all": "^4.1.3",
    "node-sass-chokidar": "^1.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "precommit": "lint-staged",
    "start-js": "PORT=3000 react-scripts start",
    "start": "npm run apply-config-local && npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "build-local": "npm run apply-config-local && npm run build",
    "apply-config-local": "copy ./src/config/env/.local.params.js ./src/config/parameters.js",
    "build-staging": "npm run apply-config-staging && npm run build",
    "apply-config-staging": "copy ./src/config/env/.staging.params.js ./src/config/parameters.js",
    "build-prod": "npm run apply-config-prod && npm run build",
    "apply-config-prod": "copy ./src/config/env/.production.params.js ./src/config/parameters.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}